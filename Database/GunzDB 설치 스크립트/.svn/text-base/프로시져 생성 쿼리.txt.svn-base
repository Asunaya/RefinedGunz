		-- Item 판매 로그 추가
		INSERT INTO ItemPurchaseLogByBounty (ItemID, CID, Date, Bounty, CharBounty, Type)
		VALUES (@ItemID, @CID, GETDATE(), @Price, @CharBP, '판매')
		IF 0 <> @@ERROR BEGIN
			ROLLBACK TRAN
			RETURN (-1)
		END

		SELECT 1 as Ret
	COMMIT TRAN

	RETURN 1
END

