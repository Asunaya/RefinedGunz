	SELECT  sql
	FROM #spaceusedTb
	ORDER BY idx
	OPEN SQL_cursor
	FETCH NEXT FROM SQL_cursor 
	INTO @sql
	WHILE @@FETCH_STATUS = 0
	BEGIN
	   exec (@sql)
	   FETCH NEXT FROM SQL_cursor 
	   INTO @sql
	END
	CLOSE SQL_cursor
	DEALLOCATE SQL_cursor

